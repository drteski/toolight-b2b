{
	"name": "toolight-b2b",
	"version": "1.0.0",
	"description": "A blank template to get started with Payload 3.0",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"build": "cross-env NODE_OPTIONS=--no-deprecation next build",
		"dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
		"devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
		"deploy:local": "cross-env TZ='UTC' NODE_OPTIONS=\"--max-old-space-size=6144 --no-deprecation\" next build && pm2 startOrRestart ecosystem.config.js --env local",
		"deploy:dev": "cross-env TZ='UTC' NODE_OPTIONS=\"--max-old-space-size=6144 --no-deprecation\" next build && pm2 startOrRestart ecosystem.config.js --env development",
		"deploy:prod": "cross-env TZ='UTC' NODE_OPTIONS=\"--max-old-space-size=6144 --no-deprecation\" next build && pm2 startOrRestart ecosystem.config.js --env production",
		"generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
		"generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
		"lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
		"payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
		"start": "cross-env NODE_OPTIONS=--no-deprecation next start",
		"docker:up": "docker-compose up -d",
		"docker:down": "docker-compose down"
	},
	"dependencies": {
		"@formatjs/intl-localematcher": "^0.6.1",
		"@hookform/resolvers": "^5.1.1",
		"@jsx-email/plugin-inline": "^1.0.1",
		"@jsx-email/plugin-minify": "^1.0.2",
		"@jsx-email/plugin-pretty": "^1.0.0",
		"@payloadcms/db-postgres": "^3.40.0",
		"@payloadcms/next": "^3.40.0",
		"@payloadcms/payload-cloud": "^3.40.0",
		"@payloadcms/plugin-search": "^3.40.0",
		"@payloadcms/richtext-lexical": "^3.40.0",
		"@payloadcms/richtext-slate": "^3.40.0",
		"@payloadcms/translations": "^3.40.0",
		"@payloadcms/ui": "^3.40.0",
		"@radix-ui/react-accordion": "^1.2.11",
		"@radix-ui/react-dialog": "^1.1.14",
		"@radix-ui/react-label": "^2.1.7",
