		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
		"cross-env": "^7.0.3",
		"embla-carousel-autoplay": "^8.6.0",
		"embla-carousel-react": "^8.6.0",
		"google-auth-library": "^9.15.1",
		"google-spreadsheet": "^4.1.4",
		"graphql": "^16.8.1",
		"jsx-email": "^2.7.2",
